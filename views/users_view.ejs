     <script>
document.addEventListener("DOMContentLoaded", () => {
  const uploadButton = document.querySelector(".upload-btn");
  const fileInput = document.querySelector("#file-input");

  if (!uploadButton || !fileInput) {
    console.error("Missing one or more required elements.");
    return;
  }

  uploadButton.addEventListener("click", async (event) => {
    event.preventDefault();

    const file = fileInput.files[0];
    if (!file) {
      alert("Please select a file.");
      return;
    }

    const formData = new FormData();
    formData.append("file", file);

    try {
      const response = await fetch("/users/upload", {
        method: "POST",
        body: formData,
      });

      const data = await response.json(); // Parse JSON once

      if (!response.ok) {
        throw new Error(data.message || `Upload failed: ${response.status}`);
      }

    //   console.log("Server Response:", data);
      alert(data.message || "File uploaded successfully!");
    } catch (error) {
      console.error("Upload error:", error);
      alert("Failed to upload file: " + error.message);
    }
  });
});



document.addEventListener("click", function (event) {
  if (event.target.classList.contains("delete")) {
    let id = event.target.dataset.id;

    Swal.fire({
      title: "Are you sure?",
      text: "This action cannot be undone!",
      icon: "warning",
      showCancelButton: true,
      confirmButtonText: "Yes, delete it!",
    }).then((result) => {
      if (result.isConfirmed) {
        fetch("/users/delete", {
          method: "POST",
          headers: {
            "Content-Type": "application/json",
          },
          body: JSON.stringify({ id: id }),
        })
          .then((response) => response.json())
          .then((data) => {
            Swal.fire({
              title: "Deleted!",
              text: data.message,
              icon: "success",
              allowOutsideClick: false, // Prevents closing without clicking OK
            }).then(() => {
              window.location.reload(true) ;// Redirect to another page
            });
          })
          .catch((error) => {
            Swal.fire("Error!", "An error occurred", "error");
          });
      }
    });
  }
});


      


$(document).on("click", ".logout", function (event) {
  event.stopPropagation(); // Prevent unintended event bubbling

  Swal.fire({
    title: "Are you sure?",
    text: "You will be logged out!",
    icon: "warning",
    showCancelButton: true,
    confirmButtonText: "Yes, logout!",
  }).then((confirmation) => {
    if (!confirmation.isConfirmed) return;

    Swal.fire({
      title: "Logging out...",
      text: "Please wait",
      allowOutsideClick: false,
      showConfirmButton: false,
      didOpen: () => {
        Swal.showLoading();
      },
    });

    $.ajax({
      url: "/users/logout",
      type: "POST",
      contentType: "application/json",
      success: function (result) {
        Swal.fire("Logged out!", "You have been logged out successfully.", "success").then(() => {
          window.location.href = "/users/login"; // Redirect to login page after logout
        });
      },
      error: function (xhr) {
        console.error("Logout Error:", xhr.responseText);
        Swal.fire("Error!", xhr.responseJSON?.error || "An error occurred", "error");
      },
    });
  });
});



